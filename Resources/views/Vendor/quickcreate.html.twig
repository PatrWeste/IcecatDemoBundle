<form id="pim_icecatdemo_vendor_quickcreate" method="POST" class="form-horizontal" data-title="{{ 'Create a vendor'|trans }}" data-button-submit="{{ 'Create'|trans }}" data-button-cancel="{{ 'Cancel'|trans }}" action="{{ path('pim_icecatdemo_vendor_quickcreate') }}" {{ form_enctype(form) }}>

    {{ JSFV(form) }}

    {% if form.vars.errors|length %}
        <div class="alert alert-error">
            {{ form_errors(form) }}
        </div>
    {% endif %}

    <div class="row-fluid">
        {{ form_row(form.code) }}
        {{ form_row(form.label) }}
        {{ form_row(form._token) }}
    </div>


</form>
